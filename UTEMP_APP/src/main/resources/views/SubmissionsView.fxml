<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.palexdev.materialfx.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox id="root" alignment="CENTER" VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/22"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.una.programmingIII.utemp_app.controllers.SubmissionsViewController">
    <children>
        <HBox alignment="CENTER" prefHeight="500.0" spacing="10.0" VBox.vgrow="ALWAYS">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0"/>
            </padding>
            <children>
                <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                    <children>
                        <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="15.0"
                              VBox.vgrow="ALWAYS">
                            <children>
                                <MFXTextField id="mfx-text-filed" fx:id="findByIdTxtF" floatMode="ABOVE"
                                              maxWidth="1.7976931348623157E308" promptText="ASSIGMENT ID HERE: "
                                              HBox.hgrow="ALWAYS"/>
                                <MFXButton id="mfx-small-button" onAction="#onActionFindByIDBtn" prefHeight="40.0"
                                           text="FIND BY COURSE ID" HBox.hgrow="ALWAYS"/>
                            </children>
                        </HBox>
                        <TableView fx:id="table" tableMenuButtonVisible="true" VBox.vgrow="ALWAYS">
                            <columns>
                                <TableColumn fx:id="idC" maxWidth="1.7976931348623157E308" prefWidth="63.73333299160004"
                                             resizable="false" text="ID"/>
                                <TableColumn fx:id="assignmentC" maxWidth="1.7976931348623157E308"
                                             prefWidth="75.20002746582031" resizable="false" text="asignacion"/>
                                <TableColumn fx:id="studentC" maxWidth="1.7976931348623157E308"
                                             prefWidth="84.79998779296875" resizable="false" text="estudiante"/>
                                <TableColumn fx:id="gradeC" prefWidth="96.80001831054688" text="grade"/>
                                <TableColumn fx:id="infoC" prefWidth="92.79998779296875" text="vacio"/>
                            </columns>
                        </TableView>
                        <HBox alignment="CENTER" prefHeight="74.0" prefWidth="398.0" spacing="5.0">
                            <children>
                                <HBox alignment="CENTER_LEFT" spacing="10.0" HBox.hgrow="ALWAYS">
                                    <children>
                                        <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0"
                                              HBox.hgrow="ALWAYS">
                                            <children>
                                                <MFXButton id="mfx-small-button" onAction="#onActionReloadPageBtn"
                                                           prefHeight="35.0" text="RELOAD PAGE"/>
                                            </children>
                                        </HBox>
                                        <MFXButton id="mfx-small-button" fx:id="prevPageBtn"
                                                   onAction="#onActionPrevPageBtn" text="PREVIOUS" HBox.hgrow="ALWAYS"/>
                                        <Label id="label-number" fx:id="pageNumberLbl" text="1" HBox.hgrow="ALWAYS"/>
                                        <MFXButton id="mfx-small-button" fx:id="nextPageBtn"
                                                   onAction="#onActionNextPageBtn" text="NEXT" HBox.hgrow="ALWAYS"/>
                                    </children>
                                </HBox>
                            </children>
                        </HBox>
                    </children>
                    <padding>
                        <Insets bottom="5.0" left="10.0" right="10.0" top="10.0"/>
                    </padding>
                </VBox>
                <Separator orientation="VERTICAL" prefHeight="470.0" prefWidth="14.0"/>
                <VBox alignment="TOP_CENTER" spacing="35.0" HBox.hgrow="ALWAYS">
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </padding>
                    <children>
                        <Label id="label" alignment="CENTER" prefHeight="18.0" prefWidth="482.0"
                               text="submission MANAGEMENT" textAlignment="CENTER" VBox.vgrow="ALWAYS">
                            <VBox.margin>
                                <Insets bottom="10.0"/>
                            </VBox.margin>
                        </Label>
                        <HBox prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <MFXButton id="mfx-small-button" onAction="#onActionClearFieldsBtn" text="CLEAR FIELDS"
                                           HBox.hgrow="ALWAYS"/>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="34.0" prefWidth="458.0" spacing="15.0">
                            <children>
                                <MFXTextField id="mfx-text-filed" fx:id="courseAssignmentTxtF" editable="false"
                                              floatMode="ABOVE" maxWidth="1.7976931348623157E308"
                                              promptText="COURSE ASSIGNMENT:" selectable="false" HBox.hgrow="ALWAYS"/>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" layoutX="20.0" layoutY="84.0" prefHeight="46.0" prefWidth="458.0"
                              spacing="15.0">
                            <children>
                                <MFXTextField id="mfx-text-filed" fx:id="studentTextF" floatMode="ABOVE"
                                              maxWidth="1.7976931348623157E308" promptText="Student"
                                              HBox.hgrow="ALWAYS"/>
                                <MFXTextField id="mfx-text-filed" fx:id="gradeTxtF" floatMode="ABOVE"
                                              maxWidth="1.7976931348623157E308" promptText="grade" HBox.hgrow="ALWAYS"/>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="168.0" maxHeight="1.7976931348623157E308"
                              maxWidth="1.7976931348623157E308" prefHeight="50.0" prefWidth="438.0" spacing="15.0"
                              VBox.vgrow="ALWAYS">
                            <VBox.margin>
                                <Insets/>
                            </VBox.margin>
                            <children>
                                <MFXTextField id="mfx-text-filed" fx:id="commentaryTxtF" floatMode="ABOVE"
                                              maxWidth="1.7976931348623157E308" prefHeight="32.0" prefWidth="484.0"
                                              promptText="Commentary"/>
                            </children>
                        </HBox>
                        <MFXTextField id="mfx-text-filed" fx:id="fileUploadPathTxtF" floatMode="ABOVE"
                                      maxWidth="1.7976931348623157E308" promptText="filePath"/>
                        <HBox alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308"
                              maxWidth="1.7976931348623157E308" prefHeight="50.0" prefWidth="438.0" spacing="15.0">
                            <children>
                                <MFXButton id="mfx-small-button-important" onAction="#onActionUploadFileBtn"
                                           text="UPLOAD FILE" HBox.hgrow="ALWAYS"/>
                                <MFXButton id="mfx-small-button-important" onAction="#onActionDownloadFileBtn"
                                           text="DOWNLOAD FILE" HBox.hgrow="ALWAYS">
                                    <HBox.margin>
                                        <Insets/>
                                    </HBox.margin>
                                </MFXButton>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="46.0" prefWidth="470.0" spacing="15.0" VBox.vgrow="ALWAYS">
                            <children>
                                <HBox prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                                    <children>
                                        <MFXButton id="mfx-small-button-important" onAction="#onActionDeleteBtn"
                                                   text="DELETE" HBox.hgrow="ALWAYS"/>
                                    </children>
                                </HBox>
                                <MFXButton id="mfx-small-button-important" onAction="#onActionUpdateBtn" text="UPDATE"
                                           HBox.hgrow="ALWAYS"/>
                                <MFXButton id="mfx-small-button-important" onAction="#onActionCreateBtn" text="CREATE"
                                           HBox.hgrow="ALWAYS"/>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308"
                              maxWidth="1.7976931348623157E308" prefHeight="50.0" prefWidth="438.0" spacing="15.0"/>
                        <HBox alignment="CENTER" prefHeight="152.0" prefWidth="387.0" spacing="15.0"
                              VBox.vgrow="ALWAYS">
                            <children>
                                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                                    <children>
                                        <MFXButton id="mfx-small-button" fx:id="backBtn" text="BACK"
                                                   HBox.hgrow="ALWAYS"/>
                                    </children>
                                </HBox>
                            </children>
                            <VBox.margin>
                                <Insets bottom="15.0"/>
                            </VBox.margin>
                        </HBox>
                    </children>
                </VBox>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</VBox>
