                        <HBox prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <MFXButton id="mfx-small-button" maxHeight="50.0" maxWidth="120.0"
                                           onAction="#onActionClearFieldsBtn" prefHeight="50.0" prefWidth="100.0"
                                           text="CLEAR FIELDS" HBox.hgrow="ALWAYS"/>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="34.0" prefWidth="458.0" spacing="15.0">
                            <children>
                                <MFXTextField id="mfx-text-filed" fx:id="courseAssigmentTxtF" editable="false"
                                              floatMode="ABOVE" maxHeight="50.0" maxWidth="1.7976931348623157E308"
                                              prefHeight="50.0" prefWidth="395.0" promptText="COURSE ASSIGNMENT:"
                                              selectable="false" HBox.hgrow="ALWAYS"/>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" layoutX="20.0" layoutY="84.0" prefHeight="46.0" prefWidth="458.0"
                              spacing="15.0">
                            <children>
                                <MFXTextField id="mfx-text-filed" fx:id="studentTextF" floatMode="ABOVE"
                                              maxHeight="50.0" maxWidth="1.7976931348623157E308" prefHeight="50.0"
                                              prefWidth="395.0" promptText="ASSIGMENT TITLE:" text="Student"
                                              HBox.hgrow="ALWAYS"/>
                                <MFXTextField id="mfx-text-filed" fx:id="gradeTxtF" floatMode="ABOVE" maxHeight="50.0"
                                              maxWidth="1.7976931348623157E308" prefHeight="50.0" prefWidth="395.0"
                                              promptText="ASSIGNMENT DESCRIPTION:" text="grade" HBox.hgrow="ALWAYS"/>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="168.0" maxHeight="1.7976931348623157E308"
                              maxWidth="1.7976931348623157E308" prefHeight="50.0" prefWidth="438.0" spacing="15.0"
                              VBox.vgrow="ALWAYS">
                            <VBox.margin>
                                <Insets/>
                            </VBox.margin>
                            <children>
                                <MFXTextField id="mfx-text-filed" fx:id="commentaryTxtF" floatMode="ABOVE"
                                              maxHeight="50.0" maxWidth="1.7976931348623157E308" prefHeight="29.0"
                                              prefWidth="463.0" promptText="ASSIGMENT TITLE:" text="Commentary"/>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="46.0" prefWidth="470.0" spacing="15.0" VBox.vgrow="ALWAYS">
                            <children>
                                <HBox prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                                    <children>
                                        <MFXButton id="mfx-small-button-important" maxHeight="50.0" maxWidth="120.0"
                                                   onAction="#onActionDeleteBtn" prefHeight="50.0" prefWidth="100.0"
                                                   text="DELETE" HBox.hgrow="ALWAYS"/>
                                    </children>
                                </HBox>
                                <MFXButton id="mfx-small-button-important" maxHeight="50.0" maxWidth="200.0"
                                           onAction="#onActionUpdateBtn" prefHeight="50.0" prefWidth="100.0"
                                           text="UPDATE" HBox.hgrow="ALWAYS"/>
                                <MFXButton id="mfx-small-button-important" maxHeight="50.0" maxWidth="200.0"
                                           onAction="#onActionCreateBtn" prefHeight="50.0" prefWidth="100.0"
                                           text="CREATE" HBox.hgrow="ALWAYS"/>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="152.0" prefWidth="387.0" spacing="15.0"
                              VBox.vgrow="ALWAYS">
                            <children>
                                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                                    <children>
                                        <MFXButton id="mfx-small-button" fx:id="backBtn" maxHeight="50.0"
                                                   maxWidth="120.0" prefHeight="50.0" prefWidth="120.0" text="BACK"
                                                   HBox.hgrow="ALWAYS"/>
                                    </children>
                                </HBox>
                                <MFXButton id="mfx-small-button-important" layoutX="247.0" layoutY="11.0"
                                           maxHeight="50.0" maxWidth="200.0" onAction="#onActionDownloadFileBtn"
                                           prefHeight="50.0" prefWidth="129.0" text="DOWNLOAD FILE" HBox.hgrow="ALWAYS">
                                    <HBox.margin>
                                        <Insets/>
                                    </HBox.margin>
                                </MFXButton>
                                <MFXButton id="mfx-small-button-important" maxHeight="50.0" maxWidth="200.0"
                                           onAction="#onActionUploadFileBtn" prefHeight="50.0" prefWidth="111.0"
                                           text="UPLOAD FILE" HBox.hgrow="ALWAYS"/>
                            </children>
                            <VBox.margin>
                                <Insets bottom="15.0"/>
                            </VBox.margin>
                        </HBox>
                    </children>
                </VBox>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</VBox>
