                            <children>
                                <MFXTextField id="mfx-text-filed" fx:id="findByIdTxtF" floatMode="ABOVE"
                                              maxHeight="40.0" maxWidth="1.7976931348623157E308" prefHeight="40.0"
                                              prefWidth="242.0" promptText="ASSIGMENT ID HERE: " HBox.hgrow="ALWAYS"/>
                                <MFXButton id="mfx-small-button" maxHeight="40.0" maxWidth="150.0"
                                           onAction="#onActionFindByIDBtn" prefHeight="40.0" prefWidth="107.0"
                                           text="FIND BY COURSE ID" HBox.hgrow="ALWAYS"/>
                            </children>
                        </HBox>
                        <TableView fx:id="table" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                   prefHeight="530.0" prefWidth="410.0" tableMenuButtonVisible="true"
                                   VBox.vgrow="ALWAYS">
                            <columns>
                                <TableColumn fx:id="idC" maxWidth="1.7976931348623157E308" prefWidth="63.73333299160004"
                                             resizable="false" text="ID"/>
                                <TableColumn fx:id="assignmentC" maxWidth="1.7976931348623157E308"
                                             prefWidth="75.20002746582031" resizable="false" text="asignacion"/>
                                <TableColumn fx:id="studentC" maxWidth="1.7976931348623157E308"
                                             prefWidth="84.79998779296875" resizable="false" text="estudiante"/>
                                <TableColumn fx:id="gradeC" prefWidth="96.80001831054688" text="grade"/>
                                <TableColumn fx:id="infoC" prefWidth="92.79998779296875" text="vacio"/>
                            </columns>
                        </TableView>
                        <HBox alignment="CENTER" prefHeight="74.0" prefWidth="398.0" spacing="5.0">
                            <children>
                                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="10.0"
                                      HBox.hgrow="ALWAYS">
                                    <children>
                                        <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0"
                                              HBox.hgrow="ALWAYS">
                                            <children>
                                                <MFXButton id="mfx-small-button" maxHeight="35.0"
                                                           onAction="#onActionReloadPageBtn" prefHeight="35.0"
                                                           prefWidth="130.0" text="RELOAD PAGE"/>
                                            </children>
                                        </HBox>
                                        <MFXButton id="mfx-small-button" fx:id="prevPageBtn" maxHeight="35.0"
                                                   maxWidth="90.0" onAction="#onActionPrevPageBtn" prefWidth="90.0"
                                                   text="PREVIOUS" HBox.hgrow="ALWAYS"/>
                                        <Label id="label-number" fx:id="pageNumberLbl" text="1" HBox.hgrow="ALWAYS"/>
                                        <MFXButton id="mfx-small-button" fx:id="nextPageBtn" maxHeight="35.0"
                                                   maxWidth="90.0" onAction="#onActionNextPageBtn" prefWidth="90.0"
                                                   text="NEXT" HBox.hgrow="ALWAYS"/>
                                    </children>
                                </HBox>
                            </children>
                        </HBox>
                    </children>
                    <padding>
                        <Insets bottom="5.0" left="10.0" right="10.0" top="10.0"/>
                    </padding>
                </VBox>
                <Separator orientation="VERTICAL" prefHeight="470.0" prefWidth="14.0"/>
